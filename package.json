{
  "name": "vue-ie",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.19.0",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "local": "cross-env NODE_ENV=local vue-cli-service serve",
    "dev": "cross-env NODE_ENV=development vue-cli-service serve",
    "prod": "cross-env NODE_ENV=production vue-cli-service serve",
    "build": "cross-env NODE_ENV=development vue-cli-service build",
    "build:prod": "cross-env NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "v-calendar": "^2.4.1",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.14",
    "vue-custom-scrollbar": "^1.4.4",
    "vue-draggable-resizable": "^2.3.0",
    "vue-lottie": "^0.2.1",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.5.4",
    "vue-svg-loader": "^0.16.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/composition-api": "^1.7.1",
    "babel-loader": "^9.1.2",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss-css-variables": "^0.18.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-gap-properties": "^3.0.5",
    "postcss-import": "^15.0.1",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    },
    "plugins": [
      "html",
      "standard",
      "vue"
    ]
  },
  "browserslist": [
    "> 0.2%",
    "IE 10",
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 ie version"
  ]
}
